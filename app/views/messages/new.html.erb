<% title t(:create_object, :object => t(:message, :scope => [ :content ])) %>

<% form_for :message, :url => objects_path do |f| %>
  <%= render :partial => 'edit', :locals => { :f => f, :message => @message } %>
  
  <%= f.hidden_field(:to_user_id) %>
  
  <%= f.submit_with_cancel(:submit => t(:create)) %>
<% end %>
