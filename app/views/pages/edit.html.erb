<% title t(:create_object, :object => t(:page, :scope => [ :content ])) %>

<%= render_heading(t(:create_object, :object => t(:page, :scope => [ :content ]))) %>

<% form_for :page, :url => object_path, :html => { :method => :put } do |f| %>
  <%= render :partial => 'pages/edit', :locals => { :f => f, :page => @page } %>

  <%= f.submit t(:save) %>
<% end %>

<%= render_page_actions([ { :allow => link_to_back } ]) %>
