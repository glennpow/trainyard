<% title h(@wiki.name) %>

<%= render_indexer(@articles_indexer) %>

<% if is_editor_of?(@wiki) %>
  <% page_action icon_link_to(:edit, t(:edit), edit_object_path) %>
  <% page_action icon_link_to(:add_large, t(:create_object, :object => t(:article, :scope => [ :content ])), new_wiki_article_path(@wiki)) %>
<% end %>
