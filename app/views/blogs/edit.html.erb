<% title t(:edit_object, :object => t(:blog, :scope => [ :blogs ])) %>

<% form_for :blog, :url => object_path, :html => { :method => :put } do |f| %>
  <%= render :partial => 'edit', :locals => { :f => f, :blog => @blog } %>

  <%= f.submit t(:save) %>
<% end %>

<%= render_page_actions([ { :allow => link_to_back } ]) %>
