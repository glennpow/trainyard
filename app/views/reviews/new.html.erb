<% title t(:reviews_of_object, :scope => [ :content ], :count => 1, :object => @review.resource.name) %>

<% form_for :review, :url => objects_path do |f| %>
  <%= render :partial => 'edit', :locals => { :f => f, :review => @review } %>
  
  <%= f.hidden_field(:resource_type) %>
  <%= f.hidden_field(:resource_id) %>
  <%= f.hidden_field(:user_id) %>

  <%= f.submit_with_cancel(:submit => t(:create)) %>
<% end %>
