body {
  background: <%= theme[:body].background %>;
  color: <%= theme[:base].font_color %>;
}

pre {
  background: <%= theme[:body].background %>;
}

.header, .footer {
  background: <%= theme[:primary].background %>;
  color: <%= theme[:primary].font_color %>;
}

.header ul li, .footer ul li {
  border-left-color: <%= theme[:primary].link_color %>;
}

.header a, .footer a {
  color: <%= theme[:primary].link_color %>;
}

.header a:hover, .footer a:hover {
  color: <%= theme[:primary].link_hover_color %>;
}

.main {
  background: <%= theme[:base].background %>;
  border-color: <%= theme[:primary].background_color %>;
}

.content, .header-content, .footer-content, .intro-content {
  background: <%= theme[:base].background %>;
  color: <%= theme[:base].font_color %>;
}

a, .header-content a, .footer-content a, .intro-content a {
  color: <%= theme[:base].link_color %>;
}

a:hover, .header-content a:hover, .footer-content a:hover, .intro-content a:hover {
  color: <%= theme[:base].link_hover_color %>;
}

.navigation {
  background: <%= theme[:secondary].background %>;
  color: <%= theme[:secondary].font_color %>;
}

.navigation ul li {
  border-left-color: <%= theme[:secondary].link_color %>;
}

.navigation a {
  color: <%= theme[:secondary].link_color %>;
  text-decoration: none;
}

.navigation a:hover {
  color: <%= theme[:secondary].link_hover_color %>;
}

.show-field .label {
}

.show-field .value {
  font-weight: bold;
}

.show-field-values {
  border: 1px dotted <%= theme[:secondary].background_color %>;
  display: table;
  width: auto;
  margin: 0px auto 5px auto;
  padding: 10px;
}

.show-field-values td {
  padding: 0px 5px;
}

.show-field-list {
  list-style: disc inside;
  text-align: left;
}

.show-field-list .label {
}

.show-text-area {
  border: 1px dotted <%= theme[:secondary].background_color %>;
}

.heading {
  border-bottom: 2px solid <%= theme[:secondary].background_color %>;
}

.heading .actions, .show-field .actions {
  font-size: smaller;
  display: inline;
  color: #666;
}

.heading .actions a, .show-field .actions a {
  color: #669;
}

.heading .actions a:hover, .show-field .actions a:hover {
  color: #336;
}

.fieldWithErrors {
  background-color: #ffdddd;
  border: 1px dotted red;
  color: #880000;
}

.form-section {
}

.required-field-flag {
  width: 14px;
  height: 14px;
  background: transparent url('/plugin_assets/trainyard/images/icons/required.jpg') no-repeat;
}

.actions a {
}

.page-actions {
  border-bottom: 1px dotted <%= theme[:secondary].background_color %>;
  margin-bottom: 10px;
}

.page-actions a {
}

.page-actions a:hover {
}

.indexer .paginate {
  background: <%= theme[:secondary].background %>;
  color: <%= theme[:secondary].font_color %>;
  border: 1px solid <%= theme[:primary].background_color %>;
}
  
table.indexer-table {
  border-collapse: collapse;
  background: <%= theme[:primary].background %>;
  border-bottom: 1px solid <%= theme[:primary].background_color %>;
}

th.indexer-th, tfoot.indexer-tfoot td {
  color: <%= theme[:primary].font_color %>;
  font-weight: normal;
}

tfoot.indexer-tfoot td {
  border-top: 1px solid <%= theme[:secondary].background_color %>;
}

th.indexer-th a, tfoot.indexer-tfoot td a {
  color: <%= theme[:primary].link_color %>;
}

th.indexer-th a:hover, tfoot.indexer-tfoot td a:hover {
  color: <%= theme[:primary].link_hover_color %>;
}

td.indexer-td {
  border-left: 1px dotted <%= theme[:secondary].background_color %>;
  border-right: 1px dotted <%= theme[:secondary].background_color %>;
}

tr.indexer-trh {
  color: <%= theme[:primary].font_color %>;
}

tr.indexer-trh a {
  color: <%= theme[:primary].link_color %>;
}

tr.indexer-trh a:hover {
  color: <%= theme[:primary].link_hover_color %>;
}

tr.indexer-tr.odd {
  background: <%= theme[:base].background %>;
  border-bottom: 1px dotted <%= theme[:secondary].background_color %>;
}

tr.indexer-tr.even {
  background: <%= theme[:base].background %>;
  border-bottom: 1px dotted <%= theme[:secondary].background_color %>;
}

tr.indexer-tr.clickable:hover {
  background: <%= theme[:highlight].background %>;
  color: <%= theme[:highlight].font_color %>;
}

tr.indexer-tr.clickable:hover a {
  color: <%= theme[:highlight].link_color %>;
}

tr.indexer-tr.clickable:hover a:hover {
  color: <%= theme[:highlight].link_hover_color %>;
}

td.indexer-td.indexer-check-box-td {
  padding: 0;
  max-width: 5px;
}

td.indexer-td.results-row-td .blurb {
  padding-top: 10px;
}

.sort-handle {
  cursor: move;
  border: 1px dotted <%= theme[:base].font_color %>;
  padding: 5px;
  white-space: nowrap;
}

.theme-element-select {
  border: 1px inset <%= theme[:secondary].background_color %>;
  padding: 5px;
  margin: 5px 40px 20px 10px;
}

ul.theme-element-select-list {
}

ul.theme-element-select-list li.theme-element-select-item {
  list-style-type: none;
  background: transparent url('/images/icons/sublist.png') no-repeat left 5px;
  padding: 2px 2px 2px 20px;
}

.theme-preview-overlay {
  background: transparent;
	<%= alpha_css(0.5) %>
}

td.searcher-td {
  border-bottom: 1px dotted <%= theme[:secondary].background_color %>;
}

td.searcher-td.searcher-left-td {
  font-weight: bold;
  vertical-align: top;
  width: 200px;
}

.results-make {
  font-weight: bold;
}

.results-model {
  font-size: large;
  font-weight: bold;
}

.results-header {
  font-weight: bold;
  margin-right: 10px;
}

table.comparer-table {
  border-collapse: separate;
  border: none;
}

td.comparer-td {
  border-left: 1px dotted <%= theme[:secondary].background_color %>;
}

td.comparer-td.first {
  border-left: none;
}

tr.comparer-tr.odd {
  background: <%= theme[:base].background %>;
}

tr.comparer-tr.even {
  background: <%= theme[:secondary].background %>;
}

.comparer .manufacturer-name {
  color: #8f8f8f;
}

.comparer .product-name {
  font-size: larger;
  color: <%= theme[:primary].background_color %>;
}

.comparer .product-description {
  color: #797677;
}

.comparer .product-details {
  font-weight: bold;
}

.comparer .feature-name {
  font-weight: bold;
}

.comparer .feature-description {
  font-size: smaller;
}

.comparer .separator {
  width: 100%;
  height: 1px;
}

.comparer .separator.top {
  margin-bottom: 10px;
}

.comparer .separator.bottom {
  margin-top: 10px;
}

.offer-flag {
  min-width: 49px;
  height: 16px;
  background: transparent url('/plugin_assets/trainyard/images/icons/offer.jpg') no-repeat top right;
}

.rating-on {
  width: 14px;
  height: 14px;
  background: transparent url('/plugin_assets/trainyard/images/icons/star_full.png') no-repeat;
}

.rating-off {
  width: 14px;
  height: 14px;
  background: transparent url('/plugin_assets/trainyard/images/icons/star_empty.png') no-repeat;
}

.rating-info {
  font-size: smaller;
  text-align: center;
  margin-top: 0px;
}

.rating-info a {
  color: #8f8f8f;
}

.article {
  background: <%= theme[:base].background %>;
  margin: 10px 0px;
}

.tabberlive .tabbertabhide {
  display: none;
}

.tabber {
}

.tabberlive {
  margin-top: 1em;
}

ul.tabbernav {
  margin: 0;
  padding: 3px 0;
  border-bottom: 1px solid #778;
  font: bold 12px Verdana, sans-serif;
}

ul.tabbernav li {
  list-style: none;
  margin: 0;
  display: inline;
}

ul.tabbernav li a {
  background: <%= theme[:secondary].background %>;
  padding: 3px 0.5em;
  margin-left: 3px;
  border: 1px solid <%= theme[:primary].background_color %>;
  border-bottom: none;
  text-decoration: none;
}

ul.tabbernav li a:link {
  color: <%= theme[:secondary].link_color %>;
}

ul.tabbernav li a:visited {
  color: <%= theme[:secondary].link_color %>;
}

ul.tabbernav li a:hover {
  color: <%= theme[:secondary].link_hover_color %>;
}

ul.tabbernav li.tabberactive a {
  color: <%= theme[:primary].background_color %>;
  background: <%= theme[:base].background %>;
  border-bottom: 1px solid <%= theme[:base].background_color %>;
}

ul.tabbernav li.tabberactive a:hover {
}

.tabberlive .tabbertab {
  padding: 5px;
  border: 1px solid <%= theme[:primary].background_color %>;
  border-top: 0;
}

a.hint-mark {
  text-decoration: none;
}

a.hint-mark:before {
  border: 2px dotted <%= theme[:primary].link_color %>;
  color: <%= theme[:primary].link_color %>;
  content: "?";
  padding: 0px 4px 0px 4px;
}

a.hint-mark:hover:before {
  border: 2px dotted <%= theme[:base].link_hover_color %>;
  color: <%= theme[:base].link_hover_color %>;
  content: "?";
  padding: 0px 4px 0px 4px;
}

.feature-type-summary {
  padding: 10px;
}

.feature-type-summary .feature-type-name {
  font-weight: bold;
  font-size: x-large;
}

.feature-type-summary .feature-type-description {
  padding-left: 10px;
}

.feature-type-summary .feature-name {
  font-weight: bold;
  padding-top: 10px;
  padding: 5px 0px;
  font-size: larger;
}

.feature-type-summary .feature-name.highlighted {
  background: <%= theme[:base].background %>;
  color: <%= theme[:base].font_color %>;
  border: 1px dotted <%= theme[:secondary].background_color %>;
}

.feature-type-summary .feature-description {
  padding-left: 10px;
}

.feature-type-summary .feature-description.highlighted {
  background: <%= theme[:base].background %>;
  color: <%= theme[:base].font_color %>;
  border: 1px dotted <%= theme[:secondary].background_color %>;
}

cite {
  font-size: smaller;
}

.filter-bar {
  background: <%= theme[:secondary].background %>;
  color: <%= theme[:secondary].font_color %>;
  border: 1px solid <%= theme[:primary].background_color %>;
  padding-left: 20px;
}

.filter-bar a {
  color: <%= theme[:secondary].link_color %>;
}

.filter-bar a:hover {
  color: <%= theme[:secondary].link_hover_color %>;
}

.breadcrumbs {
  background: <%= theme[:secondary].background %>;
  color: <%= theme[:secondary].font_color %>;
}

.breadcrumbs a {
  color: <%= theme[:secondary].link_color %>;
}

.breadcrumbs a:hover {
  color: <%= theme[:secondary].link_hover_color %>;
}

.post-header {
  background: <%= theme[:primary].background %>;
  color: <%= theme[:primary].font_color %>;
  height: 20px;
}

.listed {
  background: <%= theme[:secondary].background_color %>;
  color: <%= theme[:secondary].font_color %>;
}

.listed a {
  color: <%= theme[:secondary].link_color %>;
}

.listed a:hover {
  color: <%= theme[:secondary].link_hover_color %>;
}

.listed-image {
  border: 1px solid <%= theme[:primary].background_color %>;
}

.listed-descriptor {
  width: 150px;
}

.text-area-preview {
  border: 1px dotted <%= theme[:secondary].background_color %>;
  padding: 5px;
  margin: 5px;
}

.blog-contents {
	background: <%= theme[:secondary].background %>;
  border: 1px solid <%= theme[:primary].background_color %>;
}

.medias {
	background: <%= theme[:secondary].background %>;
	border: 2px solid #000;
}

.guru-points {
  color: #5f5;
}

.no-guru-points {
}

.map {
  border: 1px solid <%= theme[:primary].background_color %>;
}