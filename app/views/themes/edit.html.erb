<% title t(:edit_object, :object => t(:theme, :scope => [ :themes ])) %>

<%= render_heading(t(:edit_object, :object => t(:theme, :scope => [ :themes ]))) %>

<% form_for :theme, :url => object_path, :html => { :method => :put } do |f| %>
  <%= render :partial => 'edit', :locals => { :f => f, :theme => @theme } %>

  <%= f.submit t(:save) %>
<% end %>

<%= render_page_actions([ { :allow => link_to_back } ]) %>
