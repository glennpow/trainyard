<% title t(:create_object, :object => t(:group, :scope => [ :authenticate ])) %>

<% form_for :group, :url => objects_path do |f| %>
  <%= render :partial => 'edit', :locals => { :f => f, :group => @group, :parent_groups => @parent_groups } %>

  <%= f.submit t(:create) %>
<% end %>

<%= render_page_actions([ { :allow => link_to_back } ]) %>
